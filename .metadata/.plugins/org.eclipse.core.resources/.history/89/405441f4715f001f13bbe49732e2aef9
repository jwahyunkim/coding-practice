package Main;

import java.util.*;
import java.io.*;

public class aaaa {
	public static void main(String[] args) {
		
		Scanner scr = new Scanner(System.in);
		int n = scr.nextInt();
		
		for(int i=0;i<n;i++) {
			for(int j=0;j<i+1;j++) {
				System.out.println("*");
				
			}
			System.out.println("\n");
		}
	}
}
