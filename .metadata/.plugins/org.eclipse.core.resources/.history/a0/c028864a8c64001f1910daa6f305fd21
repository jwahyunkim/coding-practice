package practice;

import java.util.*;

public class Practice2 {
	public static void main(String[] args) {

		int col=3;
		for (int i = 1; i < 10; i++) {
			for (int j = 2; j < col+2; j++) {
				System.out.print(j + "x" + i + "=" + i * j);
				System.out.print("\t");
			}
			System.out.print("\n");

		}
	}
}
