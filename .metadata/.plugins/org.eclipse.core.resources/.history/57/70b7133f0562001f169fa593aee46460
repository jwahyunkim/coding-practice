package edu;

public class tem {
public static void main(String[] args) {
	char letter = 'A';
	char [] alphabet = new char[26];          //배열 생성 
	for (int i=0;i<alphabet.length;i++) {
		alphabet[i]=(char)(letter + i);         // 생성 된 배열에 값 집어 넣기
	}
	for (int i=0;i<alphabet.length;i++) {
		System.out.println(alphabet[i]);
	}		
	
//		 (char c : alphabet) {
//		System.out.println(c);
//	}		
}
}

