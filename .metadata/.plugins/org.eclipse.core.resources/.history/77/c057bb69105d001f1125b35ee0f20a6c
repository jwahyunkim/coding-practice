package Baskjoon;
import java.util.*;
public class probelm14681 {
	public static void main(String[] args) {
		
		Scanner scr = new Scanner(System.in);
		int A = scr.nextInt();
		int B = scr.nextInt();
		int Quadrant=0;
		if (A>0&&B>0) {
			Quadrant =1;
			
		}else if(A<0&&B>0) {
			Quadrant =2;
		}else if (A<0&&B<0) {
			Quadrant =3;			
		}else if (A>0&&B<0) {
			Quadrant =4;
		} 
		System.out.println(Quadrant);
		scr.close();
	}

}
