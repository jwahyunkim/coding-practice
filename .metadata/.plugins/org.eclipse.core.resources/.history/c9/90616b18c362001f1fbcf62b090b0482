package 입출력과사칙연산;
import java.util.*;
public class problem10926_equals {

	public static void main(String[] args) {
		String A = "joonas";
		String B = "baekjoon";
		
		Scanner scanner= new Scanner(System.in);

		String ID = scanner.nextLine();
		if (ID.equals(A)) {
			ID=A+"??!";			
		}
		else if (ID.equals(B)) {
			ID=B+"??!";			
			}
		else {
				
		}
		System.out.println(ID);
		
		scanner.close();
		

	}

}
