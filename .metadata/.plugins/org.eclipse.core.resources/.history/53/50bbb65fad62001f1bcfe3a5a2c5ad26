package edu;

public class Test040배열 {
	public static void main(String[] args) {
		int[][] arr = new int[5][5];
		int num = 0;
		int sum = 0;

		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
				arr[i][j] = num++;
				if ( i >= j) {
					sum+=arr[i][j];
				}
			}
		}System.out.println("합계 : "+sum);
	}
}
