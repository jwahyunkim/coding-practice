package edu;
import java.util.*;
import java.io.*;
public class tem {
	public static void main(String[] args) {
		int [] score= {95,85,100,78,98};
		int sum=0;
		double avg;
		int max =0;
		int min = 999;
		
		for(int i=0;i<score.length;i++) {
			sum+=i;
			if(max<score[i]) {
				max=score[i];
			}
			if(min>score[i]) {
				min=score[i];
			}
		}
		avg=sum/score.length;
		System.out.println(sum);
		System.out.println(avg);
		System.out.println(max);
		System.out.println(min);
	}
}

