package Baskjoon;
import java.util.*;
public class problem10430 {

	public static void main(String[] args) {
		
		
		
		Scanner scr = new Scanner(System.in);
		int A = scr.nextInt();
		int B = scr.nextInt();
		int C = scr.nextInt();
	
		
		
		int P1 = (A+B)%C;
		int P2 = ((A%C) + (B%C))%C;
		int P3 = (A*B)%C;
		int P4 = ((A%C)*(B%C))%C;
		
		System.out.println(P1);
		System.out.println(P2);
		System.out.println(P3);
		System.out.println(P4);
		
		scr.close();
		

	}

}
