import java.util.*;
public class problem1000_사칙연산 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("입력하세요");
		int A = scanner.nextInt();
		int B = scanner.nextInt();
		
		System.out.println(A+B);
		
		scanner.close();

	}

}
